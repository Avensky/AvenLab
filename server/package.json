  },
  "devDependencies": {
    "@types/express": "^4.17.17",
    "typescript": "^5.3.3",
    "vite": "^5.0.0",
    "vite-plugin-top-level-await": "^1.6.0",
    "vite-plugin-wasm": "^3.5.0"
  }
